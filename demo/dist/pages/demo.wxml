
    <view class="home-page">
        <block  wx:if="{{type == 'seltype'}}">
          <view class="we-container {{directionClass}}">
            <view class="wc-title">
              <view class="{{activeTileNow === 0 ? 'wc-title-active' : ''}}">情绪红包</view>
              <view class="{{activeTileNow === 1 ? 'wc-title-active' : ''}}">语音红包</view>
              <view class="{{activeTileNow === 2 ? 'wc-title-active' : ''}}">男女红包</view>
            </view>
            <view class="we-wrapper" bindtouchstart="touchstart" bindtouchmove="touchmove" bindtouchend="touchend" animation="{{animationData}}">
              <!-- #EC7D46 -->
              <view bindtap="slideTap" data-tag="emotionPacket" class="we-slide {{activeConNow === 0 ? 'wc-con-active' + activeConNow : ''}}" style="background: url({{imgData[0].url}}) no-repeat; background-size: cover;">
                <!-- background: url({{imgData[0 ].url}}) no-repeat; -->
                <view class="packet-text">
                  <view class="pt-txt1">点击生成情绪红包</view>
                  <view class="pt-txt2">小伙伴们说对口令就能获得随机金额</view>
                </view>
              </view>
              <!-- #E42644 -->
              <view bindtap="slideTap" data-tag="normalPacket" class="we-slide {{activeConNow === 1 ? 'wc-con-active' + activeConNow : ''}}" style="background: url({{imgData[1].url}}) no-repeat; background-size: cover;">
                <!-- background: url({{imgData[1].url}}) no-repeat; -->
                <view class="packet-text">
                  <view class="pt-txt1">点击生成语音红包</view>
                  <view class="pt-txt2">小伙伴们说对口令就能获得随机金额</view>
                </view>
              </view>
              <!-- #3279E7 -->
              <view bindtap="slideTap" data-tag="genderPacket" class="we-slide {{activeConNow === 2 ? 'wc-con-active' + activeConNow : ''}}" style="background: url({{imgData[2].url}}) no-repeat; background-size: cover;">
                <!-- background: url({{imgData[2].url}}) no-repeat; -->
                <view class="packet-text">
                  <view class="pt-txt1">点击生成男女生红包</view>
                  <view class="pt-txt2">小伙伴们说对口令就能获得随机金额</view>
                </view>
              </view>
            </view>
          </view>
        </block>
        
    <view class="nav">
        <view data-type="seltype" bindtap="$nav$changeType" class="{{'nav-item '+ ($nav$type!='seltype'?'':'nav-item-select')}}">
            <image data-type="seltype" class="nav-item-icon" src="{{'../images/'+($nav$type!='seltype'?'tab_icon_red_pocket_normal@3x.png':'tab_icon_red_pocket_selected@3x.png')}}"/> 发红包
        </view>
        <view data-type="extract" bindtap="$nav$changeType" class="{{'nav-item '+ ($nav$type!='extract'?'':'nav-item-select')}}">
            <image data-type="extract" class="nav-item-icon" src="{{'../images/'+($nav$type!='extract'?'tab_icon_withdraw_normal@3x.png':'tab_icon_withdraw_selected@3x.png')}}"/> 余额提现
        </view>
        <view data-type="record" bindtap="$nav$changeType" class="{{'nav-item '+ ($nav$type!='record'?'':'nav-item-select')}}">
            <image data-type="record" class="nav-item-icon" src="{{'../images/'+($nav$type!='record'?'tab_icon_record_normal@3x.png':'tab_icon_record_selected@3x.png')}}"/> 我的记录
        </view>
        <view data-type="question" bindtap="$nav$changeType" class="{{'nav-item '+ ($nav$type!='question'?'':'nav-item-select')}}">
            <image data-type="question" class="nav-item-icon" src="{{'../images/'+($nav$type!='question'?'tab_icon_question_normal@3x.png':'tab_icon_question_seclected@3x.png')}}"/> 常见问题
        </view>
    </view>

    </view>